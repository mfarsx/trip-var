    }));
    /* istanbul ignore next */
    cov_29e8z9g9k7().s[37]++;
    throw new ValidationError(`Validation failed: ${errorMessages.join(', ')}`, errorDetails);
  } else
  /* istanbul ignore next */
  {
    cov_29e8z9g9k7().b[6][1]++;
  }
  cov_29e8z9g9k7().s[38]++;
  next();
