      (cov_2ltyoc94al().s[73]++, Array.isArray(req.files) ?
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[35][0]++, req.files) :
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[35][1]++, [req.files]));

      // Check number of files
      /* istanbul ignore next */
      cov_2ltyoc94al().s[74]++;
      if (files.length > maxFiles) {
        /* istanbul ignore next */
        cov_2ltyoc94al().b[36][0]++;
        const error =
        /* istanbul ignore next */
        (cov_2ltyoc94al().s[75]++, new ValidationError(`Maximum ${maxFiles} file(s) allowed`));
        /* istanbul ignore next */
        cov_2ltyoc94al().s[76]++;
        return next(error);
      } else
      /* istanbul ignore next */
      {
        cov_2ltyoc94al().b[36][1]++;
      }

      // Validate each file
      cov_2ltyoc94al().s[77]++;
      for (const file of files) {
        /* istanbul ignore next */
        cov_2ltyoc94al().s[78]++;
        // Check file size
        if (file.size > maxSize) {
          /* istanbul ignore next */
          cov_2ltyoc94al().b[37][0]++;
          const error =
          /* istanbul ignore next */
          (cov_2ltyoc94al().s[79]++, new ValidationError(`File ${file.name} is too large. Maximum size is ${maxSize / 1024 / 1024}MB`));
          /* istanbul ignore next */
          cov_2ltyoc94al().s[80]++;
          return next(error);
        } else
        /* istanbul ignore next */
        {
          cov_2ltyoc94al().b[37][1]++;
        }

        // Check file type
        cov_2ltyoc94al().s[81]++;
        if (!allowedTypes.includes(file.mimetype)) {
          /* istanbul ignore next */
          cov_2ltyoc94al().b[38][0]++;
          const error =
          /* istanbul ignore next */
          (cov_2ltyoc94al().s[82]++, new ValidationError(`File type ${file.mimetype} is not allowed. Allowed types: ${allowedTypes.join(', ')}`));
          /* istanbul ignore next */
          cov_2ltyoc94al().s[83]++;
          return next(error);
        } else
        /* istanbul ignore next */
        {
          cov_2ltyoc94al().b[38][1]++;
        }
      }
      /* istanbul ignore next */
      cov_2ltyoc94al().s[84]++;
      next();
    } catch (err) {
      /* istanbul ignore next */
      cov_2ltyoc94al().s[85]++;
      next(err);
    }
  };
};

/**
 * MongoDB ObjectId validation
 * @param {string} paramName - Parameter name to validate
 * @returns {Function} Express middleware function
 */
/* istanbul ignore next */
cov_2ltyoc94al().s[86]++;
const validateObjectId = paramName => {
  /* istanbul ignore next */
  cov_2ltyoc94al().f[17]++;
  cov_2ltyoc94al().s[87]++;
  return (req, res, next) => {
    /* istanbul ignore next */
    cov_2ltyoc94al().f[18]++;
    cov_2ltyoc94al().s[88]++;
    try {
      const id =
      /* istanbul ignore next */
      (cov_2ltyoc94al().s[89]++, req.params[paramName]);
      /* istanbul ignore next */
      cov_2ltyoc94al().s[90]++;
      if (
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[40][0]++, !id) ||
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[40][1]++, !/^[0-9a-fA-F]{24}$/.test(id))) {
        /* istanbul ignore next */
        cov_2ltyoc94al().b[39][0]++;
        const error =
        /* istanbul ignore next */
        (cov_2ltyoc94al().s[91]++, new ValidationError(`Invalid ${paramName} format`));
        /* istanbul ignore next */
        cov_2ltyoc94al().s[92]++;
        return next(error);
      } else
      /* istanbul ignore next */
      {
        cov_2ltyoc94al().b[39][1]++;
      }
      cov_2ltyoc94al().s[93]++;
      next();
    } catch (err) {
      /* istanbul ignore next */
      cov_2ltyoc94al().s[94]++;
      next(err);
    }
  };
};

/**
 * Pagination validation
 * @returns {Function} Express middleware function
 */
/* istanbul ignore next */
cov_2ltyoc94al().s[95]++;
const validatePagination = () => {
  /* istanbul ignore next */
  cov_2ltyoc94al().f[19]++;
  cov_2ltyoc94al().s[96]++;
  return (req, res, next) => {
    /* istanbul ignore next */
    cov_2ltyoc94al().f[20]++;
    cov_2ltyoc94al().s[97]++;
    try {
      const page =
      /* istanbul ignore next */
      (cov_2ltyoc94al().s[98]++,
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[41][0]++, parseInt(req.query.page, 10)) ||
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[41][1]++, 1));
      const limit =
      /* istanbul ignore next */
      (cov_2ltyoc94al().s[99]++,
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[42][0]++, parseInt(req.query.limit, 10)) ||
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[42][1]++, 10));
      /* istanbul ignore next */
      cov_2ltyoc94al().s[100]++;
      if (page < 1) {
        /* istanbul ignore next */
        cov_2ltyoc94al().b[43][0]++;
        const error =
        /* istanbul ignore next */
        (cov_2ltyoc94al().s[101]++, new ValidationError('Page must be a positive integer'));
        /* istanbul ignore next */
        cov_2ltyoc94al().s[102]++;
        return next(error);
      } else
      /* istanbul ignore next */
      {
        cov_2ltyoc94al().b[43][1]++;
      }
      cov_2ltyoc94al().s[103]++;
      if (
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[45][0]++, limit < 1) ||
      /* istanbul ignore next */
      (cov_2ltyoc94al().b[45][1]++, limit > 100)) {
        /* istanbul ignore next */
        cov_2ltyoc94al().b[44][0]++;
        const error =
        /* istanbul ignore next */
        (cov_2ltyoc94al().s[104]++, new ValidationError('Limit must be between 1 and 100'));
        /* istanbul ignore next */
        cov_2ltyoc94al().s[105]++;
        return next(error);
      } else
      /* istanbul ignore next */
      {
        cov_2ltyoc94al().b[44][1]++;
      }
      cov_2ltyoc94al().s[106]++;
      req.pagination = {
        page,
        limit,
        skip: (page - 1) * limit
      };
      /* istanbul ignore next */
      cov_2ltyoc94al().s[107]++;
      next();
    } catch (err) {
      /* istanbul ignore next */
      cov_2ltyoc94al().s[108]++;
      next(err);
    }
  };
};
