    }, 'Notifications marked as read successfully'));
  } catch (err) {
    /* istanbul ignore next */
    cov_17zdutiz24().s[29]++;
    error('Error marking notifications as read', {
      error: err.message,
      userId: req.user?.id
    });
    /* istanbul ignore next */
    cov_17zdutiz24().s[30]++;
    next(err);
  }
