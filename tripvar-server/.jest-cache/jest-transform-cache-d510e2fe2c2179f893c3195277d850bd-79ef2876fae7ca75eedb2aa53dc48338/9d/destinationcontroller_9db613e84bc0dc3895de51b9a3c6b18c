        cov_eu6n0ec2m().b[5][1]++;
      }

      // If guests count is provided
      cov_eu6n0ec2m().s[20]++;
      if (guests) {
        /* istanbul ignore next */
        cov_eu6n0ec2m().b[6][0]++;
        cov_eu6n0ec2m().s[21]++;
        // In a real app, you would filter based on capacity
        // For now, we'll just log it
        info(`Search request for ${guests} guests`);
      } else
      /* istanbul ignore next */
      {
        cov_eu6n0ec2m().b[6][1]++;
      }

      // Add search conditions to query if any exist
      cov_eu6n0ec2m().s[22]++;
      if (searchConditions.length > 0) {
        /* istanbul ignore next */
        cov_eu6n0ec2m().b[7][0]++;
        cov_eu6n0ec2m().s[23]++;
        query.$or = searchConditions;
      } else
      /* istanbul ignore next */
      {
        cov_eu6n0ec2m().b[7][1]++;
      }
      const destinations =
      /* istanbul ignore next */
      (cov_eu6n0ec2m().s[24]++, await Destination.find(query));

      // Log the search for analytics purposes
      /* istanbul ignore next */
      cov_eu6n0ec2m().s[25]++;
      info('Search performed', {
        category,
        featured,
        search,
        from,
        to,
        date,
        guests,
        resultsCount: destinations.length
      });
      /* istanbul ignore next */
      cov_eu6n0ec2m().s[26]++;
      res.status(200).json({
        success: true,
        data: {
          destinations
        },
        message: 'Destinations retrieved successfully'
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_eu6n0ec2m().s[27]++;
      next(error);
    }
  },
