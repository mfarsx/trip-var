// Get notification by ID
/* istanbul ignore next */
cov_17zdutiz24().s[42]++;
const getNotificationById = async (req, res, next) => {
  /* istanbul ignore next */
  cov_17zdutiz24().f[3]++;
  cov_17zdutiz24().s[43]++;
  try {
    const {
      notificationId
    } =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[44]++, req.params);
    const userId =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[45]++, req.user.id);
    const notification =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[46]++, await Notification.findOne({
      _id: notificationId,
      user: userId
    }).populate('booking', 'bookingReference checkInDate checkOutDate totalAmount').populate('destination', 'title location imageUrl'));
    /* istanbul ignore next */
    cov_17zdutiz24().s[47]++;
    if (!notification) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[7][0]++;
      cov_17zdutiz24().s[48]++;
      throw new NotFoundError('Notification not found');
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[7][1]++;
    }

    // Mark as read if not already read
    cov_17zdutiz24().s[49]++;
    if (!notification.isRead) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[8][0]++;
      cov_17zdutiz24().s[50]++;
      notification.isRead = true;
      /* istanbul ignore next */
      cov_17zdutiz24().s[51]++;
      notification.readAt = new Date();
      /* istanbul ignore next */
      cov_17zdutiz24().s[52]++;
      await notification.save();
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[8][1]++;
    }
    cov_17zdutiz24().s[53]++;
    res.json(successResponse({
      notification
    }, 'Notification retrieved successfully'));
  } catch (err) {
    /* istanbul ignore next */
    cov_17zdutiz24().s[54]++;
    error('Error fetching notification', {
      error: err.message,
      notificationId: req.params.notificationId
    });
    /* istanbul ignore next */
    cov_17zdutiz24().s[55]++;
    next(err);
  }
};

// Get notification statistics
/* istanbul ignore next */
cov_17zdutiz24().s[56]++;
const getNotificationStats = async (req, res, next) => {
  /* istanbul ignore next */
  cov_17zdutiz24().f[4]++;
  cov_17zdutiz24().s[57]++;
  try {
    const userId =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[58]++, req.user.id);
    const stats =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[59]++, await Notification.aggregate([{
      $match: {
        user: userId
      }
    }, {
      $group: {
        _id: null,
        total: {
          $sum: 1
        },
        unread: {
          $sum: {
            $cond: [{
              $eq: ['$isRead', false]
            }, 1, 0]
          }
        },
        byType: {
          $push: {
            type: '$type',
            isRead: '$isRead'
          }
        },
        byPriority: {
          $push: {
            priority: '$priority',
            isRead: '$isRead'
          }
        }
      }
    }]));
    let notificationStats =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[60]++, {
      total: 0,
      unread: 0,
      byType: {},
      byPriority: {}
    });
    /* istanbul ignore next */
    cov_17zdutiz24().s[61]++;
    if (stats.length > 0) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[9][0]++;
      const stat =
      /* istanbul ignore next */
      (cov_17zdutiz24().s[62]++, stats[0]);
      /* istanbul ignore next */
      cov_17zdutiz24().s[63]++;
      notificationStats.total = stat.total;
      /* istanbul ignore next */
      cov_17zdutiz24().s[64]++;
      notificationStats.unread = stat.unread;

      // Calculate by type
      /* istanbul ignore next */
      cov_17zdutiz24().s[65]++;
      stat.byType.forEach(item => {
        /* istanbul ignore next */
        cov_17zdutiz24().f[5]++;
        cov_17zdutiz24().s[66]++;
        if (!notificationStats.byType[item.type]) {
          /* istanbul ignore next */
          cov_17zdutiz24().b[10][0]++;
          cov_17zdutiz24().s[67]++;
          notificationStats.byType[item.type] = {
            total: 0,
            unread: 0
          };
        } else
        /* istanbul ignore next */
        {
          cov_17zdutiz24().b[10][1]++;
        }
        cov_17zdutiz24().s[68]++;
        notificationStats.byType[item.type].total++;
        /* istanbul ignore next */
        cov_17zdutiz24().s[69]++;
        if (!item.isRead) {
          /* istanbul ignore next */
          cov_17zdutiz24().b[11][0]++;
          cov_17zdutiz24().s[70]++;
          notificationStats.byType[item.type].unread++;
        } else
        /* istanbul ignore next */
        {
          cov_17zdutiz24().b[11][1]++;
        }
      });

      // Calculate by priority
      /* istanbul ignore next */
      cov_17zdutiz24().s[71]++;
      stat.byPriority.forEach(item => {
        /* istanbul ignore next */
        cov_17zdutiz24().f[6]++;
        cov_17zdutiz24().s[72]++;
        if (!notificationStats.byPriority[item.priority]) {
          /* istanbul ignore next */
          cov_17zdutiz24().b[12][0]++;
          cov_17zdutiz24().s[73]++;
          notificationStats.byPriority[item.priority] = {
            total: 0,
            unread: 0
          };
        } else
        /* istanbul ignore next */
        {
          cov_17zdutiz24().b[12][1]++;
        }
        cov_17zdutiz24().s[74]++;
        notificationStats.byPriority[item.priority].total++;
        /* istanbul ignore next */
        cov_17zdutiz24().s[75]++;
        if (!item.isRead) {
          /* istanbul ignore next */
          cov_17zdutiz24().b[13][0]++;
          cov_17zdutiz24().s[76]++;
          notificationStats.byPriority[item.priority].unread++;
        } else
        /* istanbul ignore next */
        {
          cov_17zdutiz24().b[13][1]++;
        }
      });
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[9][1]++;
    }
    cov_17zdutiz24().s[77]++;
    res.json(successResponse({
      notificationStats
    }, 'Notification statistics retrieved successfully'));
  } catch (err) {
    /* istanbul ignore next */
    cov_17zdutiz24().s[78]++;
    error('Error fetching notification stats', {
      error: err.message,
      userId: req.user?.id
    });
    /* istanbul ignore next */
    cov_17zdutiz24().s[79]++;
    next(err);
  }
};

// Create notification (admin only)
/* istanbul ignore next */
cov_17zdutiz24().s[80]++;
const createNotification = async (req, res, next) => {
  /* istanbul ignore next */
  cov_17zdutiz24().f[7]++;
  cov_17zdutiz24().s[81]++;
  try {
    const {
      userId,
      title,
      message,
      type,
      priority =
      /* istanbul ignore next */
      (cov_17zdutiz24().b[14][0]++, 'medium'),
      actionUrl,
      actionText,
      expiresAt
    } =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[82]++, req.body);

    // Validate required fields
    /* istanbul ignore next */
    cov_17zdutiz24().s[83]++;
    if (
    /* istanbul ignore next */
    (cov_17zdutiz24().b[16][0]++, !userId) ||
    /* istanbul ignore next */
    (cov_17zdutiz24().b[16][1]++, !title) ||
    /* istanbul ignore next */
    (cov_17zdutiz24().b[16][2]++, !message) ||
    /* istanbul ignore next */
    (cov_17zdutiz24().b[16][3]++, !type)) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[15][0]++;
      cov_17zdutiz24().s[84]++;
      throw new ValidationError('Missing required notification information');
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[15][1]++;
    }
    const notification =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[85]++, await Notification.createNotification({
      user: userId,
      title,
      message,
      type,
      priority,
      actionUrl,
      actionText,
      expiresAt: expiresAt ?
      /* istanbul ignore next */
      (cov_17zdutiz24().b[17][0]++, new Date(expiresAt)) :
      /* istanbul ignore next */
      (cov_17zdutiz24().b[17][1]++, undefined)
    }));
    /* istanbul ignore next */
    cov_17zdutiz24().s[86]++;
    info('Notification created', {
      notificationId: notification._id,
      userId,
      type,
      priority
    });
    /* istanbul ignore next */
    cov_17zdutiz24().s[87]++;
    res.status(201).json(successResponse({
      notification
    }, 'Notification created successfully'));
  } catch (err) {
    /* istanbul ignore next */
    cov_17zdutiz24().s[88]++;
    error('Error creating notification', {
      error: err.message
    });
    /* istanbul ignore next */
    cov_17zdutiz24().s[89]++;
    next(err);
  }
};

// Get all notifications (admin only)
/* istanbul ignore next */
cov_17zdutiz24().s[90]++;
const getAllNotifications = async (req, res, next) => {
  /* istanbul ignore next */
  cov_17zdutiz24().f[8]++;
  cov_17zdutiz24().s[91]++;
  try {
    const {
      page =
      /* istanbul ignore next */
      (cov_17zdutiz24().b[18][0]++, 1),
      limit =
      /* istanbul ignore next */
      (cov_17zdutiz24().b[19][0]++, 20),
      userId,
      type,
      isRead,
      priority
    } =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[92]++, req.query);

    // Build query
    const query =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[93]++, {});
    /* istanbul ignore next */
    cov_17zdutiz24().s[94]++;
    if (userId) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[20][0]++;
      cov_17zdutiz24().s[95]++;
      query.user = userId;
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[20][1]++;
    }
    cov_17zdutiz24().s[96]++;
    if (type) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[21][0]++;
      cov_17zdutiz24().s[97]++;
      query.type = type;
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[21][1]++;
    }
    cov_17zdutiz24().s[98]++;
    if (isRead !== undefined) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[22][0]++;
      cov_17zdutiz24().s[99]++;
      query.isRead = isRead === 'true';
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[22][1]++;
    }
    cov_17zdutiz24().s[100]++;
    if (priority) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[23][0]++;
      cov_17zdutiz24().s[101]++;
      query.priority = priority;
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[23][1]++;
    }

    // Calculate pagination
    const skip =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[102]++, (parseInt(page, 10) - 1) * parseInt(limit, 10));

    // Get notifications with pagination
    const notifications =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[103]++, await Notification.find(query).populate('user', 'name email').populate('booking', 'bookingReference').populate('destination', 'title location').sort({
      createdAt: -1
    }).skip(skip).limit(parseInt(limit, 10)));

    // Get total count
    const total =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[104]++, await Notification.countDocuments(query));
    /* istanbul ignore next */
    cov_17zdutiz24().s[105]++;
    res.json(successResponse({
      notifications,
      pagination: {
        current: parseInt(page, 10),
        pages: Math.ceil(total / parseInt(limit, 10)),
        total
      }
    }, 'All notifications retrieved successfully'));
  } catch (err) {
    /* istanbul ignore next */
    cov_17zdutiz24().s[106]++;
    error('Error fetching all notifications', {
      error: err.message
    });
    /* istanbul ignore next */
    cov_17zdutiz24().s[107]++;
    next(err);
  }
};

