  // Check if user can review destination
  async canUserReviewDestination(userId, destinationId) {
    /* istanbul ignore next */
    cov_20lmn17v04().f[12]++;
    cov_20lmn17v04().s[43]++;
    try {
      // Check if user has already reviewed
      const hasReviewed =
      /* istanbul ignore next */
      (cov_20lmn17v04().s[44]++, await this.repository.hasUserReviewed(userId, destinationId));
      /* istanbul ignore next */
      cov_20lmn17v04().s[45]++;
      if (hasReviewed) {
        /* istanbul ignore next */
        cov_20lmn17v04().b[2][0]++;
        cov_20lmn17v04().s[46]++;
        return {
          canReview: false,
          reason: 'User has already reviewed this destination'
        };
      } else
      /* istanbul ignore next */
      {
        cov_20lmn17v04().b[2][1]++;
      }

      // Check if user has any bookings for this destination
      const Booking =
      /* istanbul ignore next */
      (cov_20lmn17v04().s[47]++, require('../public/models/booking.model'));
      const hasBooking =
      /* istanbul ignore next */
      (cov_20lmn17v04().s[48]++, await Booking.findOne({
        user: userId,
        destination: destinationId,
        status: 'confirmed'
      }));
      /* istanbul ignore next */
      cov_20lmn17v04().s[49]++;
      if (!hasBooking) {
        /* istanbul ignore next */
        cov_20lmn17v04().b[3][0]++;
        cov_20lmn17v04().s[50]++;
        return {
          canReview: false,
          reason: 'User must have a confirmed booking to review'
        };
      } else
      /* istanbul ignore next */
      {
        cov_20lmn17v04().b[3][1]++;
      }
      cov_20lmn17v04().s[51]++;
      return {
        canReview: true
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_20lmn17v04().s[52]++;
      throw error;
    }
  }

  // Get reviews with pagination
  async getReviewsWithPagination(destinationId, page =
  /* istanbul ignore next */
  (cov_20lmn17v04().b[4][0]++, 1), limit =
  /* istanbul ignore next */
  (cov_20lmn17v04().b[5][0]++, 10)) {
    /* istanbul ignore next */
    cov_20lmn17v04().f[13]++;
    cov_20lmn17v04().s[53]++;
    try {
      const reviews =
      /* istanbul ignore next */
      (cov_20lmn17v04().s[54]++, await this.repository.findByDestination(destinationId));
      const startIndex =
      /* istanbul ignore next */
      (cov_20lmn17v04().s[55]++, (page - 1) * limit);
      const endIndex =
      /* istanbul ignore next */
      (cov_20lmn17v04().s[56]++, startIndex + limit);
      const paginatedReviews =
      /* istanbul ignore next */
      (cov_20lmn17v04().s[57]++, reviews.slice(startIndex, endIndex));
      /* istanbul ignore next */
      cov_20lmn17v04().s[58]++;
      return {
        reviews: paginatedReviews,
        pagination: {
          page,
          limit,
          total: reviews.length,
          pages: Math.ceil(reviews.length / limit)
        }
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_20lmn17v04().s[59]++;
      throw error;
    }
  }
