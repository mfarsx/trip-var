const validateRequest = (req, res, next) => {
  /* istanbul ignore next */
  cov_29e8z9g9k7().f[2]++;
  const errors =
  /* istanbul ignore next */
  (cov_29e8z9g9k7().s[29]++, []);

  // Check for validation errors
  /* istanbul ignore next */
  cov_29e8z9g9k7().s[30]++;
  if (req.validationErrors) {
    /* istanbul ignore next */
    cov_29e8z9g9k7().b[5][0]++;
    cov_29e8z9g9k7().s[31]++;
    errors.push(...req.validationErrors);
  } else
  /* istanbul ignore next */
  {
    cov_29e8z9g9k7().b[5][1]++;
  }
  cov_29e8z9g9k7().s[32]++;
  if (errors.length > 0) {
    /* istanbul ignore next */
    cov_29e8z9g9k7().b[6][0]++;
    const errorMessages =
    /* istanbul ignore next */
    (cov_29e8z9g9k7().s[33]++, errors.map(error => {
      /* istanbul ignore next */
      cov_29e8z9g9k7().f[3]++;
      cov_29e8z9g9k7().s[34]++;
      return error.msg;
    }));
    const errorDetails =
    /* istanbul ignore next */
    (cov_29e8z9g9k7().s[35]++, errors.map(error => {
      /* istanbul ignore next */
      cov_29e8z9g9k7().f[4]++;
      cov_29e8z9g9k7().s[36]++;
