// Delete notifications
/* istanbul ignore next */
cov_17zdutiz24().s[31]++;
const deleteNotifications = async (req, res, next) => {
  /* istanbul ignore next */
  cov_17zdutiz24().f[2]++;
  cov_17zdutiz24().s[32]++;
  try {
    const userId =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[33]++, req.user.id);
    const {
      notificationIds
    } =
    /* istanbul ignore next */
    (cov_17zdutiz24().s[34]++, req.body);
    /* istanbul ignore next */
    cov_17zdutiz24().s[35]++;
    if (
    /* istanbul ignore next */
    (cov_17zdutiz24().b[6][0]++, !notificationIds) ||
    /* istanbul ignore next */
    (cov_17zdutiz24().b[6][1]++, notificationIds.length === 0)) {
      /* istanbul ignore next */
      cov_17zdutiz24().b[5][0]++;
      cov_17zdutiz24().s[36]++;
      throw new ValidationError('Notification IDs are required');
    } else
    /* istanbul ignore next */
    {
      cov_17zdutiz24().b[5][1]++;
