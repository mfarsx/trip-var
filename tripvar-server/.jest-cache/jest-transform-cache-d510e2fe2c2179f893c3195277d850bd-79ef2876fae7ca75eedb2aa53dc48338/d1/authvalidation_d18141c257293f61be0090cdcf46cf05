    /* istanbul ignore next */
    (cov_29e8z9g9k7().s[9]++, today.getFullYear() - birthDate.getFullYear());
    const monthDiff =
    /* istanbul ignore next */
    (cov_29e8z9g9k7().s[10]++, today.getMonth() - birthDate.getMonth());
